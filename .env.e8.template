# E8 TRADELOCKER CREDENTIALS
# Copy this to your .env file and fill in your actual credentials

# Your E8 account email
TRADELOCKER_EMAIL=your_email@example.com

# Your E8 trading password
TRADELOCKER_PASSWORD=your_password_here

# E8 server name (usually E8-Live or E8-Demo)
TRADELOCKER_SERVER=E8-Live

# TradeLocker environment URL
# Use https://api.tradelocker.com for live E8 accounts
# Use https://demo.tradelocker.com for demo accounts
TRADELOCKER_ENV=https://api.tradelocker.com


# KEEP YOUR EXISTING OANDA CREDENTIALS (for paper trading)
OANDA_API_KEY=your_oanda_key_here
OANDA_ACCOUNT_ID=your_oanda_account_id
